{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-param-reassign": "off",
    "no-undef": "off",
    "comma-dangle": "off"
  }
}
